{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    // "lib": ["ES5", "ScriptHost"],
    "moduleResolution": "Node",
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "baseUrl": "./src",
    "outDir": "./public/types"
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}
